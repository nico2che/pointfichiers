set number              
set cursorline

set autoindent
set smartindent
set expandtab
set noswapfile
set tabstop=2
set shiftwidth=2

set ruler
set hlsearch
set incsearch
set showmatch
set ignorecase
set smartcase

set showcmd
set nobackup

set autoread
set scrolloff=10

syntax on
